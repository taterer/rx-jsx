{
  "name": "@taterer/rx-jsx",
  "version": "1.0.1",
  "description": "Reactive Extensions with JSX (UI without React)",
  "scripts": {
    "analyze": "parcel build src/index.html --reporter @parcel/reporter-bundle-analyzer",
    "build": "parcel build src/index.html --public-url ./",
    "deploy": "NODE_ENV=gh yarn build && gh-pages -d dist",
    "start:server": "ts-node server",
    "start": "parcel src/index.html"
  },
  "author": "Taylor McKay",
  "bugs": {
    "url": "https://github.com/taterer/rx-jsx/issues"
  },
  "homepage": "https://taterer.github.io/rx-jsx/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/taterer/rx-jsx.git"
  },
  "keywords": [
    "rxjs",
    "jsx",
    "react",
    "reactless",
    "ui",
    "Reactive",
    "Reactive Extensions"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.12.0",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "@babylonjs/core": "^5.4.0",
    "@babylonjs/loaders": "^5.4.0",
    "@parcel/reporter-bundle-analyzer": "^2.4.1",
    "@taterer/rxjs-debugger": "^1.0.5",
    "buffer": "^6.0.3",
    "parcel": "^2.4.1",
    "parcel-reporter-static-files-copy": "^1.3.4",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3"
  },
  "dependencies": {
    "@emotion/css": "^11.9.0",
    "babylonjs": "^5.4.0",
    "babylonjs-loaders": "^5.4.0",
    "gh-pages": "^3.2.3",
    "rxjs": "^7.5.5",
    "rxjs-spy": "^8.0.2",
    "socket.io": "^4.5.0",
    "socket.io-client": "^4.5.0",
    "uuid": "^8.3.2"
  }
}
